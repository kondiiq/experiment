{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "sourceMap": true,
    "allowJs": true,
    "rootDir": ".",
    "paths": {
      "#cds-models/*": [
        "./@cds-models/*"
      ]
    }
  },
  "include" : [
    "srv/**/*.ts",
    "test/**/*.ts"
  ]
}
